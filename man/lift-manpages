# !/bin/bash

    exec < ./man1.txt
		
    while read LINE ; do
	doclifter man1/$LINE ;
    done
    mv -f man1/*.xml xml-docs/man1/

    exec < ./man3.txt
		
    while read LINE ; do
	doclifter man3/$LINE ;
    done
    mv -f man3/*.xml xml-docs/man3/
    
    exec < ./man9.txt
		
    while read LINE ; do
	doclifter man9/$LINE ;
    done
    mv -f man9/*.xml xml-docs/man9/
## generate the man?.txt with make-txt